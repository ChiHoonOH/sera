<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-config.dtd">  
<configuration> 
  <properties resource="database/jdbc.properties"/>
  <typeAliases>
  	<typeAlias alias = "ticketOrder" type="com.movie.myapp.model.TicketOrder"/>  
  		<typeAlias alias = "movie" type="com.movie.myapp.model.Movie2"/>
  	<!-- <typeAlias alias = "subing_channelDTO" type="com.itbank.TechFarm.tftube.dto.Subing_ChannelDTO"/>  </typeAliases> -->
  	</typeAliases>
  <environments default="development"> 
      <environment id="development"> 
      <transactionManager type="JDBC"/> 
      <dataSource type="POOLED"> 
        <property name="driver" value="${jdbc.driverClassName}"/> 
        <property name="url" value="${jdbc.url}"/> 
        <property name="username" value="${jdbc.username}"/> 
        <property name="password" value="${jdbc.password}"/> 
      </dataSource> 
    </environment>    
  </environments> 
  <mappers>
     	  <mapper resource="CustomerBuyTicketMapper.xml"/>
    <!-- <mapper resource="com/itbank/TechFarm/tftube/mybatis/SearchMapper.xml"/> -->
  </mappers> 
</configuration> 

