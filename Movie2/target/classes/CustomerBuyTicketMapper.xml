<!DOCTYPE mapper 
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  

<mapper namespace="com.movie.myapp.mybatis.CustomerBuyTicketMapper">		
	
<insert id="customerBuyTicket" parameterType="ticketOrder">
insert into ticket_order_table
(order_num,customer_num,movie_list_num,time)values
(#{ticketNum},#{customerNum},#{movieListNum},#{time}) 
</insert>

<select id="selectMovie" parameterType="String" resultType="movie">
select * from movie_table where movie_name=#{movie_name}
</select>

